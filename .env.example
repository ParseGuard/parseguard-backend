# File Upload
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=52428800

# AI Integration (OLLAMA)
OLLAMA_URL=http://localhost:11434

# Logging
RUST_LOG=info,parseguard_backend=debug,tower_http=debug
